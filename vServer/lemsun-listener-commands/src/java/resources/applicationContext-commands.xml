        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.1.xsd"
        default-autowire="no">


    <bean name="table.save" class="com.lemsun.channel.command.TableSaveCommand" scope="prototype"/>
    <bean name="quit" class="com.lemsun.channel.command.QuitCommand" scope="prototype" />
    <bean name="query" class="com.lemsun.channel.command.QueryCommand" scope="prototype" />
